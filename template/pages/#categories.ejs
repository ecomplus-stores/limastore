<%
const { body } = await _.resolveRoute()
_.breadcrumbs = [{
  name: _.ecomUtils.name(body),
  link: `/${body.slug}`
}]
const { parent } = body
if (parent) {
  _.breadcrumbs.unshift({
    name: _.ecomUtils.name(parent),
    link: `/${parent.slug}`
  })
}
%>

<%
console.log('Parâmetros disponíveis:', JSON.stringify(_.query));
%>

<%- await include('@/view') %>
